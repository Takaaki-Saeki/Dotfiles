# linux command
alias la='ls -a'
alias ll='ls -l'
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'

# git
alias ga='git add .'
alias gc='git commit -v'
alias gf='git fetch'
alias gst='git status'
alias gpull='git pull origin master'
alias gpush='git push origin master'

# Open
alias oc='open -a google\ chrome'
alias ox='open -a xcode'

# bash shortcut
alias bp='open -a vim ~/.bash_profile'
alias br='open -a vim ~/.bashrc'
alias sbp='source ~/.bash_profile'


